                        <!-- android:layout_gravity="center" -->
                        <!-- android:contentDescription="@string/Blank" -->
                        <!-- android:src="@drawable/search_layout_icon" /> -->

                    </LinearLayout>
                </LinearLayout>
            </ScrollView>
            <!-- categories layout finish -->

        </LinearLayout>
        <!-- header linear layout finish -->


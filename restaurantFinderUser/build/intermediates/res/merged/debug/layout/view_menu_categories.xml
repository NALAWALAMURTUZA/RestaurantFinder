                        android:layout_marginLeft="2dp"
                        android:layout_marginRight="3dp"
                        android:layout_weight="20"
                        android:background="#162880"
                        android:gravity="center"
                        android:orientation="horizontal"
                        android:visibility="visible" >

                        <LinearLayout

                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginTop="5dp"
                    android:layout_weight="3"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:visibility="invisible" >
                </LinearLayout>
            </LinearLayout>

            <LinearLayout

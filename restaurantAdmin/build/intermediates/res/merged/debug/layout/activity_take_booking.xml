<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="10"
        android:background="@drawable/rf_supper_admin_header_background"
        android:gravity="center"
        android:orientation="horizontal" >

        <ImageView
            android:id="@+id/img_home"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:src="@drawable/rf_supper_admin_home_icon"
            android:visibility="visible" />

        <TextView
            android:id="@+id/rf_supper_admin_header_name"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="8"
            android:gravity="center"
            android:text="@string/popup_tb"
            android:textColor="@android:color/white"
            android:textSize="35sp"
            android:textStyle="bold" />

        <ImageView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:src="@drawable/rf_supper_admin_home_icon"
            android:visibility="invisible" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:padding="5dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_weight="90"
        android:background="#B3D2F3"
        android:orientation="vertical" >

        <ScrollView
            android:id="@+id/ll_registration"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="70"
            android:background="#ECECEC"
            android:visibility="visible" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="fill_parent"
                    android:orientation="horizontal"
                    android:visibility="visible" >

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="25"
                        android:orientation="vertical" >

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_marginTop="10dp"
                            android:layout_weight="4"
                            android:orientation="horizontal" >

                            <TextView
                                android:id="@+id/rf_regi_step2_tv_you"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="12"
                                android:gravity="center"
                                android:text="@string/str_rf_registration_tv_you"
                                android:textColor="#0b1b70"
                                android:textSize="20sp"
                                android:textStyle="bold"
                                android:visibility="invisible" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="13"
                            android:orientation="horizontal" >

                            <TextView
                                android:id="@+id/tv_rf_registration_your_restaurant"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:text="@string/str_rf_registration_your_restaurant"
                                android:textColor="#0b1b70"
                                android:textSize="20sp"
                                android:textStyle="bold"
                                android:visibility="invisible" />
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="50"
                        android:orientation="vertical" >

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="5"
                            android:gravity="center"
                            android:orientation="horizontal" >

                            <TextView
                                android:id="@+id/tv_rf_registration_title"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="2"
                                android:gravity="right|center"
                                android:text="@string/tv_rf_registration_title"
                                android:textAppearance="?android:attr/textAppearanceMedium" />

                            <RadioGroup
                                android:id="@+id/rg_rf_regi"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="3"
                                android:orientation="horizontal" >

                                <RadioButton
                                    android:id="@+id/rb_rf_regi_mr"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:checked="true"
                                    android:text="@string/str_rf_regi_step2_rb_mr"
                                    android:textSize="12sp" />

                                <RadioButton
                                    android:id="@+id/rb_rf_regi_mrs"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="5dp"
                                    android:layout_weight="1"
                                    android:text="@string/str_rf_regi_step2_rb_mrs"
                                    android:textSize="12sp" />

                                <RadioButton
                                    android:id="@+id/rb_rf_regi_misss"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="5dp"
                                    android:layout_weight="1"
                                    android:text="@string/str_rf_regi_step2_rb_miss"
                                    android:textSize="12sp" />
                            </RadioGroup>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_marginTop="5dp"
                            android:layout_weight="5"
                            android:gravity="center"
                            android:orientation="horizontal" >

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="2"
                                android:gravity="right|center"
                                android:text="@string/tb_surname"
                                android:textAppearance="?android:attr/textAppearanceMedium" />

                            <EditText
                                android:id="@+id/tb_surname"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="20dp"
                                android:layout_weight="3"
                                android:background="@drawable/text_box"
                                android:hint="@string/tb_enter_surname"
                                android:padding="10dp"
                                android:singleLine="true"
                                android:textSize="14sp" >

                                <!-- android:text="test" -->

                            </EditText>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_marginTop="5dp"
                            android:layout_weight="5"
                            android:gravity="center"
                            android:orientation="horizontal" >

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="2"
                                android:gravity="right|center"
                                android:hint="@string/tb_firstname"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:textColorHint="#000000" />

                            <EditText
                                android:id="@+id/tb_ed_first"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="20dp"
                                android:layout_weight="3"
                                android:background="@drawable/text_box"
                                android:ems="10"
                                android:hint="@string/tb_enter_firstname"
                                android:padding="10dp"
                                android:singleLine="true"
                                android:textSize="14sp" />
                            <!-- android:text="test" -->

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_marginTop="5dp"
                            android:layout_weight="5"
                            android:gravity="center" >

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="2"
                                android:gravity="right|center"
                                android:hint="@string/tb_email"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:textColorHint="#000000" />

                            <EditText
                                android:id="@+id/tb_ed_email"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="20dp"
                                android:layout_weight="3"
                                android:background="@drawable/text_box"
                                android:ems="10"
                                android:hint="@string/tb_enter_email"
                                android:padding="10dp"
                                android:singleLine="true"
                                android:textSize="14sp" />
                            <!-- android:text="test@gmail.com" -->

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_marginTop="5dp"
                            android:layout_weight="5"
                            android:gravity="center" >

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="2"
                                android:gravity="right|center"
                                android:hint="@string/tb_phone"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:textColorHint="#000000" />

                            <TextView
                                android:id="@+id/txv_rf_country_code"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="20dp"
                                android:layout_weight="1"
                                android:background="@drawable/text_box"
                                android:editable="false"
                                android:visibility="gone"
                                android:gravity="center"
                                android:text="+40" />

                            <EditText
                                android:id="@+id/tb_ed_phone"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="20dp"
                                android:layout_weight="3"
                                android:background="@drawable/text_box"
                                android:ems="10"
                                android:hint="@string/tb_enter_phone"
                                android:inputType="phone"
                                android:padding="10dp"
                                android:singleLine="true"
                                android:textSize="14sp" />
                            <!-- android:text="1234567890" -->

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_marginTop="5dp"
                            android:layout_weight="5"
                            android:gravity="center" >

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="2"
                                android:gravity="right|center"
                                android:hint="@string/tb_business"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:textColorHint="#000000" />

                            <EditText
                                android:id="@+id/tb_ed_bussiness"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="20dp"
                                android:layout_weight="3"
                                android:background="@drawable/text_box"
                                android:ems="10"
                                android:hint="@string/tb_enter_business"
                                android:padding="10dp"
                                android:singleLine="true"
                                android:textSize="14sp" />
                            <!-- android:text="testbussiness" -->

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_marginTop="5dp"
                            android:layout_weight="5"
                            android:gravity="center" >

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="2"
                                android:gravity="right|center"
                                android:hint="@string/tb_date"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:textColorHint="#000000" />

                            <EditText
                                android:id="@+id/tb_ed_date"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="20dp"
                                android:layout_weight="3"
                                android:background="@drawable/text_box"
                                android:editable="false"
                                android:ems="10"
                                android:hint="@string/tb_select_date"
                                android:padding="10dp"
                                android:singleLine="true"
                                android:textSize="14sp" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_marginTop="5dp"
                            android:layout_weight="5"
                            android:gravity="center" >

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="2"
                                android:gravity="right|center"
                                android:hint="@string/tb_time"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:textColorHint="#000000" />

                            <EditText
                                android:id="@+id/tb_time"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="20dp"
                                android:layout_weight="3"
                                android:background="@drawable/text_box"
                                android:editable="false"
                                android:ems="10"
                                android:hint="@string/tb_select_time"
                                android:padding="10dp"
                                android:singleLine="true"
                                android:textSize="14sp" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_marginTop="5dp"
                            android:layout_weight="5"
                            android:gravity="center" >

                            <TextView
                                android:id="@+id/rf_registration_tv_country"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="2"
                                android:gravity="right|center"
                                android:hint="@string/tb_size"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:textColorHint="#000000" />

                            <EditText
                                android:id="@+id/tb_sp_size"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="20dp"
                                android:layout_weight="3"
                                android:background="@drawable/text_box"
                                android:hint="@string/tb_number_size"
                                android:inputType="phone"
                                android:padding="10dp"
                                android:textSize="14sp" />
                            <!-- android:text="12" -->

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_marginTop="5dp"
                            android:layout_weight="5"
                            android:gravity="center"
                            android:visibility="gone" >

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="2"
                                android:gravity="right|center"
                                android:hint="@string/tb_tableno"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:textColorHint="#000000" />

                            <EditText
                                android:id="@+id/rf_registration_ed_restaurant"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="20dp"
                                android:layout_weight="3"
                                android:background="@drawable/text_box"
                                android:ems="10"
                                android:hint="@string/str_rf_registration_ed_restaurant"
                                android:padding="10dp"
                                android:singleLine="true"
                                android:textSize="14sp" />
                            <!-- android:text="12" -->

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_marginTop="5dp"
                            android:layout_weight="5"
                            android:gravity="center" >

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="2"
                                android:gravity="right|center"
                                android:hint="@string/tb_note"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:textColorHint="#000000" />

                            <EditText
                                android:id="@+id/rf_registration_ed_note"
                                android:layout_width="0dp"
                                android:layout_height="70dp"
                                android:layout_marginLeft="20dp"
                                android:layout_weight="3"
                                android:background="@drawable/text_box"
                                android:ems="10"
                                android:paddingTop="15dp"
                                android:paddingLeft="5dp"
                                android:gravity="top|left"
                                android:inputType="textMultiLine"
                                android:singleLine="true"
                                android:hint="@string/str_rf_registration_ed_special_note"
                                android:padding="10dp"
                                android:textSize="14sp" />
                            <!-- android:text="testnote" -->

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_marginTop="5dp"
                            android:layout_weight="5"
                            android:gravity="center" >

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="2"
                                android:gravity="right|center"
                                android:hint="@string/tb_status"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:textColorHint="#000000" />

                            <Spinner
                                android:id="@+id/tb_sp_status"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="20dp"
                                android:layout_weight="3"
                                android:background="@drawable/text_box"
                                android:textSize="14sp" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_marginTop="5dp"
                            android:layout_weight="5"
                            android:gravity="center" >

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="2"
                                android:gravity="right|center"
                                android:hint="@string/tb_occasion"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:textColorHint="#000000" />

                            <EditText
                                android:id="@+id/tb_sp_occasion"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="20dp"
                                android:layout_weight="3"
                                android:background="@drawable/text_box"
                                android:hint="@string/tb_enter_Occasion"
                                android:padding="10dp"
                                android:singleLine="true"
                                android:textSize="14sp" />
                            <!-- android:text="testoccasion" -->

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_marginTop="5dp"
                            android:layout_weight="5"
                            android:gravity="center"
                            android:visibility="gone" >

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="2"
                                android:gravity="right|center"
                                android:hint="@string/tb_mustendat"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:textColorHint="#000000" />

                            <EditText
                                android:id="@+id/tb_end"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="20dp"
                                android:layout_weight="3"
                                android:background="@drawable/text_box"
                                android:editable="false"
                                android:hint="@string/tb_select_time"
                                android:padding="10dp"
                                android:textSize="14sp" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_marginTop="5dp"
                            android:layout_weight="5"
                            android:gravity="center"
                            android:visibility="gone" >

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="2"
                                android:gravity="right|center"
                                android:hint="@string/tb_amount"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:textColorHint="#000000" />

                            <CheckBox
                                android:id="@+id/checkBox1"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="20dp"
                                android:layout_weight="3"
                                android:text=""
                                android:visibility="gone" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_marginTop="5dp"
                            android:layout_weight="5"
                            android:gravity="center" >

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="2"
                                android:gravity="right|center"
                                android:hint="@string/tb_meal_duration"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:textColorHint="#000000" />

                            <Spinner
                                android:id="@+id/tb_duration"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="20dp"
                                android:layout_weight="3"
                                android:background="@drawable/text_box"
                                android:entries="@array/meal_duration"
                                android:textSize="14sp" />
                            <!-- <EditText -->
                            <!-- android:id="@+id/tb_duration" -->
                            <!-- android:layout_width="0dp" -->
                            <!-- android:layout_height="wrap_content" -->
                            <!-- android:layout_marginLeft="20dp" -->
                            <!-- android:layout_weight="3" -->
                            <!-- android:background="@drawable/text_box" -->
                            <!-- android:hint="@string/tb_select_time" -->
                            <!-- android:padding="10dp" -->
                            <!-- android:textSize="14sp" -->
                            <!-- android:editable="false" /> -->
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_marginTop="5dp"
                            android:layout_weight="5"
                            android:gravity="center" >

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="2"
                                android:gravity="right|center"
                                android:hint="@string/tb_amount"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:textColorHint="#000000" />

                            <EditText
                                android:id="@+id/rf_registration_amount"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="20dp"
                                android:layout_weight="3"
                                android:background="@drawable/text_box"
                                android:ems="10"
                                android:hint="@string/str_rf_registration_ed_amount"
                                android:inputType="numberDecimal"
                                android:padding="10dp"
                                android:singleLine="true"
                                android:textSize="14sp"
                                android:text="0" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_marginTop="5dp"
                            android:layout_weight="5"
                            android:gravity="center" >

                            <TextView
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="2"
                                android:gravity="right|center"
                                android:hint="@string/tb_attached_file"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:textColorHint="#000000" />

                            <LinearLayout
                                android:id="@+id/tb_photo"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="20dp"
                                android:layout_weight="3"
                                android:background="@drawable/text_box"
                                android:gravity="center"
                                android:orientation="horizontal"
                                android:padding="5dp"
                                android:visibility="visible" >

                                <TextView
                                    android:id="@+id/txv_choosefile"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:background="@drawable/rounded_corner2"
                                    android:gravity="left|center"
                                    android:padding="5dp"
                                    android:text="@string/gs_cf"
                                    android:textColor="#ffffff"
                                    android:textSize="14sp" />

                                <TextView
                                    android:id="@+id/txv_choose_filename"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="2"
                                    android:gravity="left|center"
                                    android:paddingLeft="5dp"
                                    android:text="@string/step2_nofile"
                                    android:textSize="14sp" />
                            </LinearLayout>
                        </LinearLayout>

                        <LinearLayout

                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:text="@string/str_rf_registration_your_restaurant"
                                android:textColor="#0b1b70"
                                android:textSize="20sp"
                                android:textStyle="bold" />
                        </LinearLayout>

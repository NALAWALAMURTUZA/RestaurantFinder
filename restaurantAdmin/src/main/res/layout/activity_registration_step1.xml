                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="5dp"
                                    android:layout_weight="1"
                                    android:text="@string/str_rf_regi_step2_rb_miss"
                                    android:textSize="12sp" />
                            </RadioGroup>

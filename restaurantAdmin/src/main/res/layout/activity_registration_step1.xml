        android:layout_height="0dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="5dp"
        android:layout_weight="80"
        android:background="#B3D2F3"
        android:orientation="vertical"
        android:padding="5dp" >


<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    android:orientation="vertical" >

    <!-- Header aligned to top -->

    <LinearLayout
        android:id="@+id/rf_login_img_header"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="8"
        android:background="@drawable/rf_supper_admin_header_background"
        android:orientation="horizontal" >

        <ImageView
            android:id="@+id/rf_login_img_header_icon"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="left|center"
            android:layout_weight="3"
            android:src="@drawable/rf_supper_admin_home_icon" />

        <ImageView
            android:id="@+id/img_menu"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="left|center"
            android:layout_weight="1"
            android:src="@drawable/ic_launcher"
            android:visibility="invisible" />

        <TextView
            android:id="@+id/rf_supper_admin_txv_invoice"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="8"
            android:gravity="center"
            android:text="@string/gi_title"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="@android:color/white"
            android:textSize="35sp" />

        <ImageView
            android:id="@+id/img"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="left|center"
            android:layout_weight="2"
            android:src="@drawable/ic_launcher"
            android:visibility="invisible" />

        <ImageView
            android:id="@+id/img_logo"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="2"
            android:src="@drawable/ic_launcher"
            android:visibility="invisible" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="90"
        android:gravity="center"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="15"
            android:orientation="horizontal"
            android:padding="10dp" >

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="2"
                android:orientation="vertical" >
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:layout_weight="6"
                android:background="@drawable/rounded_corner"
                android:orientation="vertical"
                android:padding="10dp" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_marginLeft="2dp"
                    android:layout_marginRight="2dp"
                    android:layout_weight="10"
                    android:gravity="center"
                    android:orientation="horizontal" >

                    <TextView
                        android:id="@+id/txv_fromdate"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.3"
                        android:text="@string/gi_from_date"
                        android:textSize="14sp"
                        android:typeface="serif" />

                    <EditText
                        android:id="@+id/rf_invoice_ed_fromdate"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="3dp"
                        android:layout_weight="2.5"
                        android:background="@drawable/rf_text_box"
                        android:editable="false"
                        android:gravity="left|center"
                        android:hint="@string/gi_select_from_date"
                        android:paddingLeft="5dp"
                        android:textSize="12sp" />

                    <TextView
                        android:id="@+id/txv_todate"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_weight="1"
                        android:text="@string/gi_to_date"
                        android:textSize="14sp"
                        android:typeface="serif" />

                    <EditText
                        android:id="@+id/rf_invoice_ed_todate"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="2.5"
                        android:background="@drawable/rf_text_box"
                        android:editable="false"
                        android:gravity="center_vertical"
                        android:hint="@string/gi_select_to_date"
                        android:paddingLeft="5dp"
                        android:textSize="12sp" />

                    <Button
                        android:id="@+id/btn_submit"
                        style="@android:attr/buttonBarButtonStyle"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="3dp"
                        android:layout_weight="2"
                        android:background="@drawable/rf_butten_box"
                        android:gravity="center"
                        android:text="@string/gi_submit"
                        android:textColor="#FFFFFF"
                        android:textSize="14sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_weight="5"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:visibility="gone" >

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="2.2"
                        android:text="@string/rf_login_forgetpass"
                        android:textSize="10sp" />

                    <EditText
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="2"
                        android:background="@drawable/rf_text_box"
                        android:ems="10"
                        android:hint="@string/rf_login_ed_username"
                        android:inputType="text"
                        android:visibility="invisible" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="10"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:visibility="gone" >

                    <Button
                        android:id="@+id/btn"
                        style="@android:attr/buttonBarButtonStyle"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.5"
                        android:background="@drawable/rf_butten_box"
                        android:textSize="30sp"
                        android:visibility="invisible" />

                    <Button
                        style="@android:attr/buttonBarButtonStyle"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="2"
                        android:background="@drawable/rf_butten_box"
                        android:gravity="center"
                        android:text="@string/gi_submit"
                        android:textColor="#FFFFFF"
                        android:textSize="14sp" />

                    <Button
                        android:id="@+id/btn1"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.5"
                        android:background="@drawable/rf_butten_box"
                        android:textSize="30sp"
                        android:visibility="invisible" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="2"
                android:orientation="vertical" >
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="2dp"
            android:layout_marginRight="2dp"
            android:background="@drawable/rounded_corner3"
            android:gravity="left|center"
            android:orientation="horizontal" >

            <RadioGroup
                android:id="@+id/rg_in_on"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="3"
                android:orientation="horizontal" >

                <RadioButton
                    android:id="@+id/radiobtn_grabtable"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.5"
                    android:checked="true"
                    android:text="@string/gi_tg" />

                <RadioButton
                    android:id="@+id/radiobtn_online"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.5"
                    android:text="@string/gi_oo" />
            </RadioGroup>

            <AutoCompleteTextView
                android:id="@+id/autocomplete_textview_generate_invoice_search"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:background="@drawable/rounded_corner7"
                android:drawableRight="@drawable/search" >
            </AutoCompleteTextView>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="70"
            android:orientation="vertical"
            android:padding="10dp" >

            <LinearLayout
                android:id="@+id/ll_tg_raw_file"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginTop="10dp"
                android:layout_weight="10"
                android:background="#eeeeee"
                android:gravity="center"
                android:orientation="horizontal"
                android:padding="3dp"
                android:visibility="visible" >

                <TextView
                    android:id="@+id/txv_sr_no"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/roundcorner_all_booking"
                    android:gravity="center"
                    android:text="@string/gi_srno"
                    android:textColor="#000000" />

                <TextView
                    android:id="@+id/txv_restaurant_name"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:background="@drawable/roundcorner_all_booking"
                    android:gravity="center"
                    android:text="@string/gi_rest_name"
                    android:textColor="#000000" />

                <TextView
                    android:id="@+id/txv_email"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:background="@drawable/roundcorner_all_booking"
                    android:gravity="center"
                    android:text="@string/gi_email" />

                <TextView
                    android:id="@+id/txv_po"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/roundcorner_all_booking"
                    android:gravity="center"
                    android:text="@string/oo_invoice_po"
                    android:textColor="#000000" />

                <TextView
                    android:id="@+id/txv_co"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/roundcorner_all_booking"
                    android:gravity="center"
                    android:text="@string/oo_invoice_co" />

                <TextView
                    android:id="@+id/txv_rpo"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/roundcorner_all_booking"
                    android:gravity="center"
                    android:text="@string/oo_invoice_rpo" />

                <TextView
                    android:id="@+id/txv_fo"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/roundcorner_all_booking"
                    android:gravity="center"
                    android:text="@string/oo_invoice_fo"
                    android:textColor="#000000" />

                <TextView
                    android:id="@+id/txv_nswt"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/roundcorner_all_booking"
                    android:gravity="center"
                    android:text="@string/oo_invoice_nswt" />

                <TextView
                    android:id="@+id/txv_nsat"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/roundcorner_all_booking"
                    android:gravity="center"
                    android:text="@string/oo_invoice_nsat"
                    android:textColor="#000000" />

                <TextView
                    android:id="@+id/txv_cba"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/roundcorner_all_booking"
                    android:gravity="center"
                    android:text="@string/oo_invoice_cba" />

                <TextView
                    android:id="@+id/txv_cbu"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/roundcorner_all_booking"
                    android:gravity="center"
                    android:text="@string/oo_invoice_cbu" />

                <TextView
                    android:id="@+id/txv_tb"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/roundcorner_all_booking"
                    android:gravity="center"
                    android:text="@string/oo_invoice_tb"
                    android:textColor="#000000" />
                
                <TextView
                    android:id="@+id/txv_nop"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/roundcorner_all_booking"
                    android:gravity="center"
                    android:text="@string/oo_invoice_nop"
                    android:textColor="#000000" />
                
                <TextView
                    android:id="@+id/txv_bc"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/roundcorner_all_booking"
                    android:gravity="center"
                    android:text="@string/oo_invoice_bc"
                    android:textColor="#000000" />
                
                
                

                <TextView
                    android:id="@+id/txv_ulp"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="@drawable/roundcorner_all_booking"
                    android:gravity="center"
                    android:text="@string/oo_invoice_ulp" />

                <TextView
                    android:id="@+id/txv_total"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:background="@drawable/roundcorner_all_booking"
                    android:gravity="center"
                    android:text="@string/gi_total" />

                <TextView
                    android:id="@+id/txv_status"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:background="@drawable/roundcorner_all_booking"
                    android:gravity="center"
                    android:text="@string/gi_status" />
            </LinearLayout>

<!--             OO ni row file  -->
            
            <LinearLayout
                android:id="@+id/ll_oo_raw_file"

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="50"
                android:background="@drawable/rounded_corner7"
                android:orientation="horizontal" >

                <ListView
                    android:id="@+id/lv_generate_pdf"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" >
                </ListView>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginTop="10dp"
                android:layout_weight="10"
                android:gravity="center"
                android:orientation="horizontal"
                android:visibility="visible" >

                <Button
                    android:id="@+id/btn12"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.5"
                    android:background="@drawable/rf_butten_box"
                    android:textSize="30sp"
                    android:visibility="invisible" />

                <Button
                    android:id="@+id/btn_generate_pdf"
                    style="@android:attr/buttonBarButtonStyle"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.5"
                    android:background="@drawable/rf_butten_box"
                    android:text="@string/gi_generate_pdf"
                    android:textColor="#ffffff"
                    android:textSize="14sp"
                    android:visibility="visible" />

                <Button
                    style="@android:attr/buttonBarButtonStyle"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:background="@drawable/rf_butten_box"
                    android:gravity="center"
                    android:text="@string/gi_email"
                    android:textColor="#FFFFFF"
                    android:textSize="14sp"
                    android:visibility="gone" />

                <Button
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.5"
                    android:background="@drawable/rf_butten_box"
                    android:textSize="30sp"
                    android:visibility="invisible" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

</LinearLayout>
<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    android:orientation="vertical" >

    <!-- Header aligned to top -->

    <LinearLayout
        android:id="@+id/rf_login_img_header"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="10"
        android:background="@drawable/rf_supper_admin_header_background"
        android:orientation="horizontal" >

        <ImageView
            android:id="@+id/rf_login_img_header_icon"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="left|center"
            android:layout_weight="1"
            android:src="@drawable/rf_supper_admin_home_icon" />

        <ImageView
            android:id="@+id/img_menu"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="left|center"
            android:layout_weight="0"
            android:src="@drawable/ic_launcher"
            android:visibility="invisible" />

        <TextView
            android:id="@+id/rf_supper_admin_txv_home"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="7"
            android:gravity="center"
            android:text="@string/rf_supper_admin_txv_home"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="@android:color/white"
            android:textSize="35sp" />

        <ImageView
            android:id="@+id/img_refresh"
            android:layout_width="0dp"
            android:layout_weight="1"
             android:src="@drawable/img_refresh"
            android:layout_height="match_parent"
            android:text="Button"
            android:padding="10dp" />

        <ImageView
            android:id="@+id/img"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_gravity="left|center"
            android:layout_weight="0"
            android:src="@drawable/ic_launcher"
            android:visibility="invisible" />

        <ImageView
            android:id="@+id/img_popup"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:src="@drawable/menu_bar" />
    </LinearLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_weight="90" >

        <LinearLayout
            android:id="@+id/header"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginTop="10dp"
                android:layout_weight="10"
                android:orientation="horizontal" >

                <LinearLayout
                    android:id="@+id/ll_restuarant_list"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="5"
                    android:background="@drawable/rounded_corner1"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:padding="2dp" >

                    <ImageView
                        android:id="@+id/image_task_booking_icon"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:src="@drawable/rf_suuper_admin_task_booking_icon" />

                    <TextView
                        android:id="@+id/rf_supper_txv_restaurant_list"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="4"
                        android:text="@string/rf_supper_txv_restaurant_list"
                        android:textAppearance="?android:attr/textAppearanceSmall" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_manage_restuarant"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="5"
                    android:background="@drawable/rounded_corner1"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:padding="2dp" >

                    <ImageView
                        android:id="@+id/setting_icon"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:src="@drawable/rf_suuper_admin_manage_setting_icon" />

                    <TextView
                        android:id="@+id/rf_supper_txv_manage_restaurant"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="4"
                        android:text="@string/rf_supper_txv_manage_restaurant"
                        android:textAppearance="?android:attr/textAppearanceSmall" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginTop="10dp"
                android:layout_weight="10"
                android:orientation="horizontal" >

                <LinearLayout
                    android:id="@+id/ll_admin_profile"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="5"
                    android:background="@drawable/rounded_corner1"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:padding="2dp" >

                    <ImageView
                        android:id="@+id/profile_icon"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:src="@drawable/rf_suuper_admin_admin_profile_icon" />

                    <TextView
                        android:id="@+id/rf_supper_txv_admin_profile"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="4"
                        android:text="@string/rf_supper_txv_admin_profile"
                        android:textAppearance="?android:attr/textAppearanceSmall" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_manage_packages_price_name"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="5"
                    android:background="@drawable/rounded_corner1"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:padding="2dp" >

                    <ImageView
                        android:id="@+id/price_icon"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:src="@drawable/rf_suuper_admin_manage_packges_price_icon" />

                    <TextView
                        android:id="@+id/rf_supper_txv_manage_packages_price_name"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="4"
                        android:text="@string/rf_supper_txv_manage_packages_price_name"
                        android:textAppearance="?android:attr/textAppearanceSmall" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginTop="10dp"
                android:layout_weight="10"
                android:orientation="horizontal" >

                <LinearLayout
                    android:id="@+id/ll_generate_invoice"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="5"
                    android:background="@drawable/rounded_corner1"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:padding="2dp" >

                    <ImageView
                        android:id="@+id/invoice_icon"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:src="@drawable/invoice_icon" />

                    <TextView
                        android:id="@+id/rf_supper_txv_invoice"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="4"
                        android:text="@string/rf_supper_txv_invoice"
                        android:textAppearance="?android:attr/textAppearanceSmall" />
                </LinearLayout>
                
                

                <LinearLayout
                    android:id="@+id/ll_global_setting"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="5"
                    android:background="@drawable/rounded_corner1"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:padding="2dp"
                    android:visibility="visible" >

                    <ImageView
                        android:id="@+id/price_icon1"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:src="@drawable/manage_account" />

                    <TextView
                        android:id="@+id/txv"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="4"
                        android:text="@string/str_global_setting"
                        android:textAppearance="?android:attr/textAppearanceSmall" />
                </LinearLayout>
                
                
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginTop="10dp"
                android:layout_weight="10"
                android:orientation="horizontal" >

                <LinearLayout
                    android:id="@+id/ll_manage_restaurant"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="5"
                    android:background="@drawable/rounded_corner1"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:padding="2dp" >

                    <ImageView
                        android:id="@+id/img_manage_restaurant"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:src="@drawable/manage_category" />

                    <TextView
                        android:id="@+id/txt_manage_restaurant"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="4"
                        android:text="@string/pm_manage_restaurant"
                        android:textAppearance="?android:attr/textAppearanceSmall" />
                </LinearLayout>
                
                <LinearLayout
                    android:id="@+id/ll_request_update_package"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="5"
                    android:background="@drawable/rounded_corner1"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:padding="2dp"
                    android:visibility="visible" >

                    <ImageView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:src="@drawable/notification" />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="4"
                        android:text="@string/str_rquest_update_package"
                        android:textAppearance="?android:attr/textAppearanceSmall" />
                </LinearLayout>
                
                
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginTop="10dp"
                android:layout_weight="10"
                android:orientation="horizontal" >

       
                    <LinearLayout
                        android:id="@+id/ll_logout"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="5"
                        android:background="@drawable/rounded_corner1"
                        android:gravity="center"
                        android:orientation="horizontal"
                        android:padding="2dp"
                        android:visibility="visible" >

                        <ImageView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:src="@drawable/logput_icon" />

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="4"
                            android:text="@string/pm_logout"
                            android:textAppearance="?android:attr/textAppearanceSmall" />
                </LinearLayout>
                
                

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="5"
                    android:background="@drawable/rounded_corner1"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:padding="2dp"
                    android:visibility="invisible" >

                 
                </LinearLayout>
                
                
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

</LinearLayout>
<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="10"
        android:background="@drawable/rf_supper_admin_header_background"
        android:gravity="center"
        android:orientation="horizontal"
        android:visibility="visible" >

        <ImageView
            android:id="@+id/home_img"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:src="@drawable/rf_supper_admin_home_icon"
            android:visibility="visible" />

        <TextView
            android:id="@+id/rf_supper_admin_header_name"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="9"
            android:gravity="center"
            android:text="@string/str_global_setting"
            android:textColor="@android:color/white"
            android:textSize="35sp"
            android:textStyle="bold" />

        <ImageView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:src="@drawable/rf_supper_admin_home_icon"
            android:visibility="invisible" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_weight="90"
        android:background="#ECECEC"
        android:orientation="vertical"
        android:padding="5dp" >


        <ScrollView
            android:id="@+id/ll_registration"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="70"
            android:background="#ECECEC"
            android:visibility="visible" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="fill_parent"
                    android:orientation="horizontal"
                    android:visibility="visible" >

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="10"
                        android:orientation="vertical" >
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="70"
                        android:orientation="vertical" >

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_marginTop="10dp"
                            android:layout_weight="5"
                            android:gravity="center"
                            android:orientation="horizontal" >

                            <TextView
                                android:id="@+id/rf_sa_tv_fname"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="2"
                                android:gravity="right|center"
                                android:text="@string/str_sa_fname"
                                android:textAppearance="?android:attr/textAppearanceMedium" />

                            <EditText
                                android:id="@+id/rf_sa_ed_fname"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="20dp"
                                android:layout_weight="3"
                                android:background="@drawable/text_box"
                                android:hint="@string/str_sa_ed_fname"
                                android:padding="10dp"
                                android:singleLine="true"
                                android:textSize="14sp" >
                            </EditText>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_marginTop="10dp"
                            android:layout_weight="5"
                            android:gravity="center"
                            android:orientation="horizontal" >

                            <TextView
                                android:id="@+id/rf_sa_tv_lname"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="2"
                                android:gravity="right|center"
                                android:text="@string/str_sa_lname"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:textColorHint="#000000" />

                            <EditText
                                android:id="@+id/rf_sa_ed_lname"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="20dp"
                                android:layout_weight="3"
                                android:background="@drawable/text_box"
                                android:ems="10"
                                android:hint="@string/str_sa_ed_lname"
                                android:padding="10dp"
                                android:singleLine="true"
                                android:textSize="14sp" />
                        </LinearLayout>

                        
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_marginTop="10dp"
                            android:layout_weight="5"
                            android:gravity="center"
                            android:orientation="horizontal" >

                            <TextView
                                android:id="@+id/rf_sa_tv_email"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="2"
                                android:gravity="right|center"
                                android:text="@string/str_sa_email"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:textColorHint="#000000" />

                            <EditText
                                android:id="@+id/rf_sa_ed_email"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="20dp"
                                android:layout_weight="3"
                                android:background="@drawable/text_box"
                                android:ems="10"
                                android:editable="false"
                                android:padding="10dp"
                                android:singleLine="true"
                                android:textSize="14sp" />
                        </LinearLayout>
                       
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_marginTop="10dp"
                            android:layout_weight="5"
                            android:gravity="center" >

                            <TextView
                                android:id="@+id/rf_sa_tv_mini_loy"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="2"
                                android:gravity="right|center"
                                android:text="@string/str_sa_min_lp_to_redeem"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:textColorHint="#000000" />

                            <EditText
                                android:id="@+id/rf_sa_ed_mini_loy"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="20dp"
                                android:layout_weight="3"
                                android:background="@drawable/text_box"
                                android:ems="10"								android:inputType="numberDecimal"
                                android:hint="@string/str_sa_ed_min_lp_to_redeem"
                                android:padding="10dp"
                                android:singleLine="true"
                                android:textSize="14sp" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_marginTop="10dp"
                            android:layout_weight="5"
                            android:gravity="center" >

                            <TextView
                                android:id="@+id/rf_sa_tv_max_loy"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="2"
                                android:gravity="right|center"
                                android:text="@string/str_sa_max_lp_to_redeem"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:textColorHint="#000000" />

                            <EditText
                                android:id="@+id/rf_sa_ed_max_loy"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="20dp"
                                android:layout_weight="3"
                                android:inputType="numberDecimal"
                                android:background="@drawable/text_box"
                                android:hint="@string/str_sa_ed_max_lp_to_redeem"
                                android:padding="10dp"
                                android:textSize="14sp" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_marginTop="10dp"
                            android:layout_weight="5"
                            android:gravity="center" >

                            <TextView
                                android:id="@+id/rf_sa_loy_tv_lei"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="2"
                                android:gravity="right|center"
                                android:text="@string/str_sa_lp_equals_to_lei"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:textColorHint="#000000" />

                            <EditText
                                android:id="@+id/rf_sa_loy_ed_lei"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="20dp"
                                android:layout_weight="3"
                                android:background="@drawable/text_box"
                                android:ems="10"
                                android:inputType="numberDecimal"
                                android:hint="@string/str_sa_ed_lp_equals_to_lei"
                                android:padding="10dp"
                                android:singleLine="true"
                                android:textSize="14sp" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_marginTop="10dp"
                            android:layout_weight="5"
                            android:gravity="center" >

                            <TextView
                                android:id="@+id/rf_sa_lp_tv_tg_cust"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="2"
                                android:gravity="right|center"
                                android:text="@string/str_sa_lp_to_tg_customer"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:textColorHint="#000000" />

                            <EditText
                                android:id="@+id/rf_sa_lp_ed_tg_cust"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="20dp"
                                android:layout_weight="3"
                                android:inputType="numberDecimal"
                                android:background="@drawable/text_box"
                                android:hint="@string/str_sa_ed_lp_to_tg_customer"
                                android:padding="10dp"
                                android:textSize="14sp" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_marginTop="10dp"
                            android:layout_weight="5"
                            android:gravity="center" >

                            <TextView
                                android:id="@+id/rf_sa_tv_booking_time_limit"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="2"
                                android:gravity="right|center"
                                android:text="@string/str_sa_booking_time_limit"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:textColorHint="#000000" />

                            <EditText
                                android:id="@+id/rf_sa_ed_booking_time_limit"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="20dp"
                                android:layout_weight="3"
                                android:background="@drawable/text_box"
                                android:ems="10"
                                android:inputType="numberDecimal"
                                android:hint="@string/str_sa_ed_booking_time_limit"
                                android:padding="10dp"
                                android:singleLine="true"
                                android:textSize="14sp" />
                        </LinearLayout>
                        
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_marginTop="10dp"
                            android:layout_weight="5"
                            android:gravity="center" >

                            <TextView
                                android:id="@+id/rf_sa_tv_oo_percentage"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="2"
                                android:gravity="right|center"
                                android:text="@string/str_sa_oo_percentage"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:textColorHint="#000000" />

                            <EditText
                                android:id="@+id/rf_sa_ed_oo_percentage"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="20dp"
                                android:layout_weight="3"
                                android:background="@drawable/text_box"
                                android:ems="10"
                                android:inputType="numberDecimal"
                                android:hint="@string/str_sa_ed_oo_percentage"
                                android:padding="10dp"
                                android:singleLine="true"
                                android:textSize="14sp" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_marginLeft="20dp"
                            android:layout_marginRight="20dp"
                            android:layout_marginTop="10dp"
                            android:layout_weight="10"
                            android:orientation="vertical" >

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="0dp"
                                android:layout_weight="10"
                                android:gravity="center"
                                android:orientation="horizontal" >

                                <Button
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="2"
                                    android:background="#253E66"
                                    android:padding="15dp"
                                    android:text="@string/btn_update"
                                    android:textColor="#ffffff"
                                    android:visibility="invisible" />

                                <Button
                                    android:id="@+id/rf_sa_btn_update"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="6"
                                    android:background="#253E66"
                                    android:padding="15dp"
                                    android:text="@string/btn_update"
                                    android:textColor="#ffffff" />

                                <Button
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="2"
                                    android:background="#253E66"
                                    android:padding="15dp"
                                    android:text="@string/btn_update"
                                    android:textColor="#ffffff"
                                    android:visibility="invisible" />
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="15"
                        android:orientation="vertical" >
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </ScrollView>

     
    </LinearLayout>
    
</LinearLayout>